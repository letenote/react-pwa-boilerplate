{
  "name": "react-pwa-boilerplate",
  "version": "1.0.0",
  "description": "First init project and install - commands are:",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "build-sw": "node ./src/workbox/v5/sw-build.js",
    "dev": "NODE_ENV=development webpack serve --mode=development",
    "prod-v5": "rm -rf dist && GENERATE_SOURCEMAP=false NODE_ENV=production WORKBOX_VERSION=5 webpack --mode=production && npm run build-sw",
    "prod-v6": "rm -rf dist && GENERATE_SOURCEMAP=false NODE_ENV=production WORKBOX_VERSION=6 webpack --mode=production",
    "serve-v5": "rm -rf dist && npm run prod-v5 && serve -s -p 6969 dist",
    "serve-v6": "rm -rf dist && npm run prod-v6 && serve -s -p 6969 dist",
    "svg": "svgr -d src/components/icons src/components/icons/svgs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letenote/react-pwa-boilerplate.git"
  },
  "keywords": [],
  "author": "",
